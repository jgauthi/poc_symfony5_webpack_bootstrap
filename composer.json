{
  "name": "jgauthi/poc_symfony5_webpack_bootstrap",
  "description": "POC for use webpack + bootstrap in symfony",
  "type": "project",
  "license": "GPL-3.0-only",
  "require": {
    "php": ">=8.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-simplexml": "*",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.10",
    "easycorp/easyadmin-bundle": "2.*",
    "knplabs/knp-paginator-bundle": "*",
    "sensio/framework-extra-bundle": "6.*",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/dotenv": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "1.*",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/intl": "*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/process": "*",
    "symfony/property-access": "*",
    "symfony/property-info": "*",
    "symfony/proxy-manager-bridge": "*",
    "symfony/security-bundle": "*",
    "symfony/serializer": "*",
    "symfony/string": "*",
    "symfony/translation": "*",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "symfony/web-link": "*",
    "symfony/webpack-encore-bundle": "1.*",
    "symfony/yaml": "*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "vich/uploader-bundle": "1.*"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "*",
    "fakerphp/faker": "1.*",
    "friendsofphp/php-cs-fixer": "3.*",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpro/grumphp-shim": "1.*",
    "phpstan/phpstan-doctrine": "0.12.*",
    "phpstan/phpstan-symfony": "0.12.*",
    "symfony/debug-bundle": "*",
    "symfony/maker-bundle": "1.*",
    "symfony/stopwatch": "*",
    "symfony/web-profiler-bundle": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.3.*"
    }
  }
}
